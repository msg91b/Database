	<queryString>
		<![CDATA[SELECT unique
     "JGMG_VIEW_TRANSACTION"."PRODUCTID",
     "JGMG_VIEW_TRANSACTION"."TITLE",
     JGMG_COSTS."PRICE",
     "JGMG_VIEW_TRANSACTION"."Sale Date",
     "JGMG_VIEW_TRANSACTION".saleID,
     "JGMG_VIEW_TRANSACTION".total,
     "JGMG_VIEW_TRANSACTION".quantity

FROM
     "WINTER342"."JGMG_PRODUCT" JGMG_PRODUCT_A INNER JOIN "WINTER342"."JGMG_COSTS" JGMG_COSTS ON JGMG_PRODUCT_A."PRODUCTID" = JGMG_COSTS."PRODUCTID"
     INNER JOIN "JGMG_VIEW_TRANSACTION" ON JGMG_PRODUCT_A."PRODUCTID" = "JGMG_VIEW_TRANSACTION"."PRODUCTID"
     AND JGMG_PRODUCT_A."TITLE" = "JGMG_VIEW_TRANSACTION"."TITLE"
WHERE
     saleID = $P{saleID}
 AND ("Sale Date" BETWEEN sDate AND to_Date(NVL(eDate, sysdate)))]]>
	</queryString>
